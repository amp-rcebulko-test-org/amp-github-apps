{
  "name": "amp-pr-deploy-bot",
  "description": "A GitHub App that deploys a PR branch when you ask it to",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML Authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "^14.0.0"
  },
  "scripts": {
    "build": "tsc ./src/*.ts --esModuleInterop",
    "dev": "nodemon",
    "start": "probot run ./src/app.js",
    "test": "jest --verbose=false"
  },
  "dependencies": {
    "@google-cloud/storage": "5.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "mime-types": "2.1.26",
    "probot": "11.4.0",
    "typescript": "4.3.2",
    "unzip-stream": "0.3.0"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/express": "4.17.2",
    "@types/jest": "26.0.23",
    "@types/mime-types": "2.1.0",
    "@types/node": "14.17.2",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "eslint": "7.28.0",
    "jest": "27.0.4",
    "nodemon": "2.0.2",
    "smee-client": "1.1.0",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
