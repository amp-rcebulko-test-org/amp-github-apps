{
  "name": "amp-bundle-size-app",
  "description": "A GitHub App that adds a bundle-size check to pull requests",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "^12.13.1"
  },
  "scripts": {
    "dev": "nodemon",
    "deploy": "gcloud --project amp-bundle-size-bot app deploy",
    "deploy-tag": "git tag 'deploy-bundle-size-'`date --utc '+%Y%m%d%H%M%S'`",
    "setup-db": "node ./setup-db.js",
    "start": "probot run ./app.js",
    "test": "jest"
  },
  "dependencies": {
    "@octokit/auth": "1.1.1",
    "@octokit/rest": "16.43.2",
    "knex": "0.95.4",
    "node-cache": "5.1.2",
    "pg": "7.18.2",
    "probot": "9.15.1",
    "sleep-promise": "8.0.1"
  },
  "devDependencies": {
    "fancy-log": "1.3.3",
    "jest": "24.9.0",
    "nock": "11.9.1",
    "nodemon": "2.0.7",
    "smee-client": "1.2.2",
    "sqlite3": "4.2.0",
    "supertest": "4.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
